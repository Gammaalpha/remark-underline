    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "babel-loader": "^8.2.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "remark": "^13.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
